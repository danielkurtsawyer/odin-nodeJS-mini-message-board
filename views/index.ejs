<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="../public/styles.css" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <div class="messagesContainer">
      <% messages.forEach((message) => { %> <%- include('messages/message',
      {user: message.user, text: message.text, date: message.added}) %> <% }) %>
    </div>
  </body>
</html>
